{
  "manifest_version": 3,
  "name": "书签搜索",
  "description": "快速搜索Chrome书签",
  "version": "1.0",
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "mac": "Command+Shift+K"
      },
      "description": "打开书签搜索"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
} 